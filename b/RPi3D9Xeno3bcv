    key <AE04> { [ 4,            currency,       dollar                                ] };
    key <AE05> { [ 5,            percent,        sterling                              ] };
    key <AE06> { [ 6,            ampersand                                             ] };
    key <AE07> { [ 7,            slash,          braceleft                             ] };
    key <AE08> { [ 8,            parenleft,      bracketleft                           ] };
    key <AE09> { [ 9,            parenright,     bracketright                          ] };
