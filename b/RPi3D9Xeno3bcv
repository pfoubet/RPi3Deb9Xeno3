    key <AB01> { [ z,            Z                                                     ] };
    key <AB02> { [ x,            X                                                     ] };
    key <AB03> { [ c,            C                                                     ] };
    key <AB04> { [ v,            V                                                     ] };
    key <AB05> { [ b,            B                                                     ] };
    key <AB06> { [ n,            N                                                     ] };
    key <AB07> { [ m,            M                                                     ] };
    key <AB08> { [ comma,        semicolon,      less                                  ] };
    key <AB09> { [ period,       colon,          greater                               ] };
    key <AB10> { [ minus,        underscore,     dead_diaeresis                        ] };

