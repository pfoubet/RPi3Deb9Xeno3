    key <BKSL> {        [       backslash,             bar,     Ukrainian_ghe_with_upturn, Ukrainian_GHE_WITH_UPTURN ] };

    key <LSGT> {        [            less,         greater,     slash,           bar            ]       };
    key	<AB01> {	[		z,		 Z,	Cyrillic_ya,     Cyrillic_YA	]	};
    key	<AB02> {	[		x,		 X,	Cyrillic_che,    Cyrillic_CHE	]	};
