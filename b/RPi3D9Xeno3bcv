	include "sun_vndr/us(sun_type6)"

    key <TLDE> { [      grave,    notsign,  brokenbar ] };

    key <AE02> { [          2,   quotedbl             ] };
    key <AE03> { [          3,   sterling             ] };
    key <AE04> { [          4,     dollar,   EuroSign ] };
    key <BKSL> { [ numbersign, asciitilde             ] };
    key <AC11> { [ apostrophe,         at             ] };
    key <LSGT> { [  backslash,        bar             ] };

	include "level3(ralt_switch)"
};

