    include "rs(latinyz)"

    name[Group1]= "Slovenian (US keyboard with Slovenian letters)";

    key <TLDE> { type[Group1]="TWO_LEVEL", [ cedilla, diaeresis ] };
};

partial alphanumeric_keys
