    key <AC09>  { [              l,              L                 ] };
    key <AC10>  { [           plus,      plusminus                 ] };
    key <AC11>  { [     dead_acute,     dead_grave                 ] };

    key <LSGT>  { [   bracketright,    bracketleft,            bar ] };
