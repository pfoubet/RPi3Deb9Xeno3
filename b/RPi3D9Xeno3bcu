    		   NoSymbol,  dead_caron ]	};
    key <BKSL>	{ [    agrave,         Agrave,     braceright, NoSymbol,
    		   NoSymbol,  dead_breve ]	};

    key <LSGT>	{ [    ugrave,     Ugrave,         degree, NoSymbol,
    		   NoSymbol,   brokenbar ]	};

