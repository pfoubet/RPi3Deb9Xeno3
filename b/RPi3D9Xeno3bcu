   key <AB09> { [   0x1000964, 0x10009FA ] };
   key <AB10> { [   0x10009AF, 0x10009DF ] };
   key <BKSL> { [   backslash, bar	 ] };

// third level with right-win
//    include "level3(lwin_switch)"

//   key <LCTL> { [   Control_L              ] };
//   key <SPCE> { [   space                  ] };

