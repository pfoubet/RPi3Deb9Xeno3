    key <AE11>  { [          slash,       question,      backslash ] };
    key <AE12>  { [         degree,     dead_tilde,   dead_cedilla ] };

    key <AD01>  { [              q,              Q                 ] };
    key <AD02>  { [              w,              W                 ] };
    key <AD03>  { [              e,              E,       EuroSign ] };
    key <AD04>  { [              r,              R,      paragraph ] };
