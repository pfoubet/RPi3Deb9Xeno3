    key <AB02>  { [              x,              X, guillemotright ] };
    key <AB03>  { [              c,              C,           cent ] };
    key <AB04>  { [              v,              V                 ] };
    key <AB05>  { [              b,              B                 ] };
    key <AB06>  { [              n,              N                 ] };
    key <AB07>  { [              m,              M,             mu ] };
    key <AB08>  { [          comma,      semicolon                 ] };
    key <AB09>  { [         period,          colon, periodcentered ] };
    key <AB10>  { [          minus,          equal                 ] };

    include "level3(ralt_switch)"
};


