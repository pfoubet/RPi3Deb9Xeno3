    key <AE02>	{ [          2,    quotedbl,          ograve,        Ograve ] };
    key <AE03>	{ [          3,    slash,          at,        VoidSymbol ] };

    key <AD03>	{ [          e,    E,          EuroSign, VoidSymbol]	};

