    // add romanian-specific letters to the German nodeadkeys layout.
    // Read the comment for de_ro !

    include "de(nodeadkeys)"
    name[Group1]="Romanian (Germany, eliminate dead keys)";

    key <AD05> { [         t,    T,           tcedilla,     Tcedilla    ] };
