partial alphanumeric_keys
xkb_symbols "phonetic-alt" {
    include "am(phonetic)"
    name[Group1]= "Armenian (alternative phonetic)";

    key <AE08> {	[ 0x1000580,  0x1000550 ]	};
    key <LatR> {	[ 0x100057c,  0x100054c ]	};

