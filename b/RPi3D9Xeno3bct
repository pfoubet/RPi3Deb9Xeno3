   key <BKSL> { [ backslash, bar, 0x100003f ] };

   key <AB01> { [ Arabic_zah, Arabic_kaf, 0x10006d2] };
   key <AB02> { [ Arabic_tah, 0x1000653 , 0x1000691 ] };
   key <AB03> { [ Arabic_zain, 0x1000698, 0x1000696 ] };
