" URL:		    http://www.cs.cmu.edu/~aleksa/personal/vim/keymap/serbian-cp1251.vim
" Last Changed: 2002 Mar 29

scriptencoding cp1251

let b:keymap_name = "serbian-cp1251"

loadkeymap
