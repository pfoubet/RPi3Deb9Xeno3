" URL:		    http://www.cs.cmu.edu/~aleksa/personal/vim/keymap/serbian-UTF-8.vim
" Last Changed: 2002 Mar 29

scriptencoding UTF-8

let b:keymap_name = "serbian-UTF-8"

loadkeymap
