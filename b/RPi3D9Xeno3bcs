	    <ESC>=<HIRA>,
	    <FK01>=<I180>,
	    <FK02>=<I163>,
	    <FK03>=<I225>,
	    <FK04>=<I152>,
	    <FK05>=<I148>,
	    <FK06>=<I179>,
	    <FK07>=<I173>,
	    <FK08>=<I172>,
	    <FK09>=<I171>,
	    <FK10>=<I121>,
	    <FK11>=<I122>,
	    <FK12>=<I123>,
	    <DELE>=<I169>
	};
    };

