<dead_tilde> <e>			: "\250"	etilde
<dead_acute> <E>			: "\311"	Eacute
<dead_acute> <e>			: "\351"	eacute
<dead_belowdot> <E>			: "\211"	Ebelowdot
<dead_belowdot> <e>			: "\251"	ebelowdot
