    indicator "Misc" { left = 6; top = 60.5; shape = "LED";
                       offColor = "grey10"; onColor = "grey10"; };
};

xkb_geometry "tm2030_MiscDiod_on" {
    indicator "Misc" { left = 6; top = 60.5; shape = "LED";
                       offColor = "green"; onColor = "green"; };
};


