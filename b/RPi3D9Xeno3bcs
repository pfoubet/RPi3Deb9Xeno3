		    <AB01>, <AB02>, <AB03>, <AB04>, <AB05>, 
		    <AB06>, <AB07>, <AB08>, <AB09>, <AB10>,
		    { <RTSH>, shape="WIDER", color="grey30" }
	    };
	};
	row {
            left = 57;
	    top = 77;
	    keys { <AE00>, <LSGT> };
	};
    }; // End of "Alpha" section

    alias <AC00> = <CAPS>;
    alias <AA00> = <LCTL>;
}; // End of "pc86" geometry

