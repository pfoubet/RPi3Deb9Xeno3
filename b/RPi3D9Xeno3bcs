<dead_diaeresis>   <Cyrillic_i>  : "и̏"
<dead_doubleacute> <Cyrillic_i>  : "и̏"
<dead_diaeresis>   <Cyrillic_I>  : "И̏"
<dead_doubleacute> <Cyrillic_I>  : "И̏"
<dead_diaeresis>   <Cyrillic_o>  : "о̏"
<dead_doubleacute> <Cyrillic_o>  : "о̏"
<dead_diaeresis>   <Cyrillic_O>  : "О̏"
<dead_doubleacute> <Cyrillic_O>  : "О̏"
<dead_diaeresis>   <Cyrillic_u>  : "у̏"
<dead_doubleacute> <Cyrillic_u>  : "у̏"
