_ldapwhoami()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -h)
            _known_hosts_real "$cur"
            return 0
            ;;
        -H)
            _ldap_uris
            return 0
            ;;
        -f|-y)
            _filedir
            return 0
            ;;
        -P)
            _ldap_protocols
            return 0
            ;;
    esac

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '$( _parse_help "$1" ) -MM -ZZ' -- "$cur" ) )
    fi
} &&
complete -F _ldapwhoami ldapwhoami

_ldappasswd()
{
    local cur prev words cword
    _init_completion || return

    case $prev in
        -h)
            _known_hosts_real "$cur"
            return 0
            ;;
        -H)
            _ldap_uris
            return 0
            ;;
        -t|-T|-y)
            _filedir
            return 0
            ;;
    esac

    if [[ "$cur" == -* ]]; then
        COMPREPLY=( $( compgen -W '$( _parse_help "$1" ) -MM -ZZ' -- "$cur" ) )
    fi
} &&
complete -F _ldappasswd ldappasswd

