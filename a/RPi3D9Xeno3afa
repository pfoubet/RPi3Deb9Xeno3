#ifndef _CRYPTO_CTR_H
#define _CRYPTO_CTR_H

#define CTR_RFC3686_NONCE_SIZE 4
#define CTR_RFC3686_IV_SIZE 8
#define CTR_RFC3686_BLOCK_SIZE 16

