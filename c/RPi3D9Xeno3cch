#ifndef _ASM_TILE_KDEBUG_H
#define _ASM_TILE_KDEBUG_H

#include <linux/notifier.h>

enum die_val {
	DIE_OOPS = 1,
	DIE_BREAK,
	DIE_SSTEPBP,
	DIE_PAGE_FAULT,
	DIE_COMPILED_BPT
};

