	__le32 reserved2[2];
	__le32 itt;
	__le32 reserved3;
	__le32 stat_sn;
	__le32 exp_cmd_sn;
	__le32 max_cmd_sn;
	__le32 reserved4;
	__le16 time2retain;
	__le16 time2wait;
	__le32 reserved5[1];
};

struct iscsi_text_request_hdr {
