#define WM8400_LLOPGALON                        0x0040  /* LLOPGALON */
#define WM8400_LLOPGALON_MASK                   0x0040  /* LLOPGALON */
#define WM8400_LLOPGALON_SHIFT                       6  /* LLOPGALON */
